{
  "extends": "man",
  "rules": {
    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn",
    "no-prototype-builtins": "off"
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  }
}
